{
  "name": "financial_network",
  "vertex_id_type" : "long",
  "edge_id_type" : "long",
  "edge_id_strategy" : "keys_as_ids",
  "vertex_providers": [
    {
      "format": "csv",
      "name": "Account",
      "key_column" : 0,
      "key_type" : "long",
      "props": [
        {
          "name": "number",
          "type": "long",
          "column": 1
        }
      ],
      "separator": " ",
      "uris": ["accounts.csv"]
    },
    {
      "format": "csv",
      "name": "Person",
      "key_column" : 0,
      "key_type" : "long",
      "props": [
        {
          "name": "name",
          "type": "string",
          "column": 1
        }
      ],
      "separator": " ",
      "uris": ["persons.csv"]
    },
    {
      "format": "csv",
      "name": "Company",
      "key_column" : 0,
      "key_type" : "long",
      "props": [
        {
          "name": "name",
          "type": "string",
          "column": 1
        }
      ],
      "separator": " ",
      "uris": ["companies.csv"]
    }
  ],
  "edge_providers": [
    {
      "format": "csv",
      "name": "transaction",
      "source_vertex_provider": "Account",
      "destination_vertex_provider": "Account",
      "key_column" : 0,
      "key_type" : "long",
      "source_column" : 1,
      "destination_column" : 2,
      "props": [
        {
          "name": "key",
          "type": "long",
          "column": 0
        },
        {
          "name": "amount",
          "type": "double",
          "column": 3
        }
      ],
      "loading": {
        "create_key_mapping": true
      },
      "separator": " ",
      "uris": ["transactions.csv"]
    },
    {
      "format": "csv",
      "name": "ownerOf1",
      "source_vertex_provider": "Person",
      "destination_vertex_provider": "Account",
      "key_column" : 0,
      "key_type" : "long",
      "source_column" : 1,
      "destination_column" : 2,
      "props": [
        {
          "name": "key",
          "type": "long",
          "column": 0
        }
      ],
      "loading": {
        "create_key_mapping": true
      },
      "separator": " ",
      "uris": ["person_ownerOf_account.csv"]
    },
    {
      "format": "csv",
      "name": "ownerOf2",
      "source_vertex_provider": "Company",
      "destination_vertex_provider": "Account",
      "key_column" : 0,
      "key_type" : "long",
      "source_column" : 1,
      "destination_column" : 2,
      "props": [
        {
          "name": "key",
          "type": "long",
          "column": 0
        }
      ],
      "loading": {
        "create_key_mapping": true
      },
      "separator": " ",
      "uris": ["company_ownerOf_account.csv"]
    },
    {
      "format": "csv",
      "name": "worksFor",
      "source_vertex_provider": "Person",
      "destination_vertex_provider": "Company",
      "key_column" : 0,
      "key_type" : "long",
      "source_column" : 1,
      "destination_column" : 2,
      "props": [
        {
          "name": "key",
          "type": "long",
          "column": 0
        }
      ],
      "loading": {
        "create_key_mapping": true
      },
      "separator": " ",
      "uris": ["person_worksFor_company.csv"]
    }
  ]
