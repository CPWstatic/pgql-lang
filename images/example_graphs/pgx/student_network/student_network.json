      "destination_column" : 2,
      "props": [
      ],
      "separator": " ",
      "uris": ["person_knows_person.csv"]
    },
    {
      "format": "csv",
      "name": "studentOf",
      "source_vertex_provider": "Person",
      "destination_vertex_provider": "University",
      "source_column" : 1,
      "destination_column" : 2,
      "props": [
      ],
      "separator": " ",
      "uris": ["person_studentOf_university.csv"]
    }
  ]
}
