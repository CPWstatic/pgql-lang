    {
      "format": "csv",
      "name": "Person",
      "key_column" : 1,
      "key_type" : "long",
      "props": [
        {
          "name": "name",
          "type": "string",
          "column": 2
        },
        {
          "name": "dob",
          "type": "local_date",
          "column": 3
        }
      ],
      "separator": " ",
      "uris": ["persons.csv"]
    },
    {
      "format": "csv",
      "name": "University",
      "key_column" : 1,
      "key_type" : "long",
